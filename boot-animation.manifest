  <assign>
    <filesystem path="/usr/bin/boot-animation" label="boot-animation" exec_label="boot-animation" />
    <filesystem path="/usr/share/sounds/poweron.ogg" label="_" />
    <filesystem path="/usr/share/sounds/poweron.wav" label="_" />
  </assign>
</manifest>
